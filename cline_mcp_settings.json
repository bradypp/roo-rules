{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [
        "get-library-docs"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": []
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "autoApprove": [
        "create_entities",
        "read_graph",
        "delete_entities",
        "create_relations",
        "add_observations"
      ],
      "disabled": false
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "BSAUfxYXObzAzU4Lc19w1xGg-BV_ua2"
      }
    },
    "task-master": {
      "command": "npx",
      "args": [
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ],
      "env": {
        "GOOGLE_API_KEY": "AIzaSyAXLs3eFDklr_DnOqvUTVsTc47fDltOHrQ",
        "TASK_MASTER_MAIN_MODEL": "gemini-2.5-flash-preview-05-20",
        "TASK_MASTER_RESEARCH_MODEL": "gemini-2.5-flash-preview-05-20",
        "TASK_MASTER_FALLBACK_MODEL": "gemini-2.5-flash-preview-05-20"
      },
      "type": "stdio",
      "disabled": true,
      "autoApprove": []
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    }
  }
}
